    <script src="~/Areas/Admin/Templates/adminpro/js/data-table/colResizable-1.5.source.js"></script>
    <script src="~/Areas/Admin/Templates/adminpro/js/data-table/bootstrap-table-export.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/Areas/Admin/Templates/adminpro/js/main.js"></script>
</body>
</html>
